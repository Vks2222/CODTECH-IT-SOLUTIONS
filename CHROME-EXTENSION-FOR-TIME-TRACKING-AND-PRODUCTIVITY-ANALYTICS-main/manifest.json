{
    "manifest_version": 3,
    "name": "Time Tracker & Productivity Analytics",
    "version": "1.0",
    "permissions": ["tabs", "storage", "history", "activeTab"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup/popup.html",
      "default_icon": "icons/icon.png"
    },
    "host_permissions": ["<all_urls>"]
  }
  